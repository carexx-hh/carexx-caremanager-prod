.select {
  position: relative;
  color:#5489FD;
}
.select:after {
  position: absolute;
  left: 135rpx;
  bottom: 0rpx;
  content: "";
  width:100rpx;
  height:6rpx;
  background:rgba(84,137,253,1);
  border-radius:10rpx 10rpx 0 0;
}
.active{
    color:#5489FD;
} 
